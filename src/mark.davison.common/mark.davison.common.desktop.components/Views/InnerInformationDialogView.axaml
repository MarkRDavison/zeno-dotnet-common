<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:mark.davison.common.client.desktop.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="mark.davison.common.client.desktop.InnerInformationDialogView"
			 x:DataType="vm:InnerInformationDialogViewModel">
	<StackPanel Margin="8" Orientation="Vertical" Spacing="16">
		<TextBlock Text="{Binding PrimaryContent}" />
		<TextBlock Text="{Binding SecondaryContent}" />
	</StackPanel>
</UserControl>
