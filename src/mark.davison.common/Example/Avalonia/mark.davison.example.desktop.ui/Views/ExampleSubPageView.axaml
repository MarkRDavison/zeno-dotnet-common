<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:mark.davison.example.desktop.ui.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="mark.davison.example.desktop.ui.ExampleSubPageView"
			 x:DataType="vm:ExampleSubPageViewModel">
	<StackPanel Orientation="Vertical"
				VerticalAlignment="Center"
				HorizontalAlignment="Center">

		<TextBlock Text="{Binding Name}"
				   FontSize="64"/>
		<Button Command="{Binding OpenChildCommand}">Open child</Button>
	</StackPanel>
</UserControl>
