<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:mark.davison.example.desktop.ui.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="mark.davison.example.desktop.ui.ExampleClientRepositoryPageView"
			 x:DataType="vm:ExampleClientRepositoryPageViewModel">


	<StackPanel Orientation="Vertical"
				VerticalAlignment="Center"
				HorizontalAlignment="Center">
		<TextBlock FontSize="32">
			<Run Text="Request: "/>
			<Run Text="{Binding Request}"/>
		</TextBlock>
		<TextBlock FontSize="32">
			<Run Text="Response: "/>
			<Run Text="{Binding Response}"/>
		</TextBlock>
	</StackPanel>
	
</UserControl>
